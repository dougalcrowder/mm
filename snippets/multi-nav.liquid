        <a href="{{ link.url }}" class="nav_link{% if link.active %} active{% endif %}{% if link.links.size > 0 %} hasChildren{% endif %}" {% if link.active %}aria-current="page"{% endif %}>
          {{ link.title }}
        </a>
        {%- if link.links.size > 0 -%}
        <ul class="sub_links">
