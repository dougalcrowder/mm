{% when 'nav_columns' %}
<div class="nav_columns flex">
  {%- if block.settings.title != blank -%}
    <header class="title">{{ block.settings.title }}</header>
  {%- endif -%}
  {%- if block.settings.col_nav_1a != blank or block.settings.col_nav_1b != blank -%}
