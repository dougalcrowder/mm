{% case block.type %}

{% when 'four_menus' %}
<div class="four_menus flex{% if block.settings.fl_title != blank %} with_t{%- endif -%}" {{ block.shopify_attributes }}>
  {% if block.settings.fl_title != blank %}
  <div class="navtitle">{{ block.settings.fl_title}}</div>
  {%- endif -%}
  {% if block.settings.link1 != blank %} 
    <a class="link1" href="{{ block.settings.link1}}">
      {{ block.settings.label1}}
    </a>
  {%- endif -%}
  {% if block.settings.link2 != blank %} 
    <a class="link2" href="{{ block.settings.link2}}">
      {{ block.settings.label2}}
    </a>
  {%- endif -%}
  {% if block.settings.link3 != blank %} 
    <a class="link3" href="{{ block.settings.link3}}">
      {{ block.settings.label3}}
    </a>
  {%- endif -%}
  {% if block.settings.link4 != blank %} 
    <a class="link4" href="{{ block.settings.link4}}">
      {{ block.settings.label4}}
    </a>
  {%- endif -%}
</div>
  
{% when 'two_menus' %}
<div class="hero_menus flex">
<div class="hero1" {{ block.shopify_attributes }}>
    {% if block.settings.link != blank %} 
    <a href="{{ block.settings.link }}">
    {%- endif -%}
    {%- if block.settings.title != blank -%}
      <strong>{{ block.settings.title | escape }}</strong>
    {%- endif -%}
    {%- if block.settings.image != blank -%}
      <div class="image">
        {%- assign image = block.settings.image -%}
        <picture>
          <source srcset="{{ image | img_url: '390x135', crop: 'center', scale: 2 }} 2x, {{ image | img_url: '390x135', crop: 'center' }} 1x" media="(min-width: 480px)" />
          <source srcset="{{ image | img_url: '390x135', scale: 2, crop: 'center' }} 2x, {{ image | img_url: '390x135', crop: 'center' }} 1x" media="(min-width: 320px)" />
          <img class="lazyimage" loading="lazy" src="{{ image | img_url: '390x135', crop: 'center' }}" alt="{{ image.alt | escape }}" width="390" height="135" />
        </picture>
      </div>
    {%- endif -%}
    {% if block.settings.link != blank %} 
    </a>
    {%- endif -%}
    
    {%- if block.settings.color1-link != blank -%}
    <div class="colour_links flex">
