    "166196159906fadd3f": {
      "type": "simple_quote",
      "settings": {
        "quote": "<p>\"Molly Mahon has a magical, modern eye for block printing. I have long been a fan of her work\"<\/p>",
        "cite": "- Cath Kidston MBE",
        "bg": "green",
        "heading_size": "medium",
        "vertical_alignment": "center",
        "alignment": "center",
        "color_scheme": "white_text",
        "bg_color_scheme": "none",
        "margin_top": "none",
        "margin_bot": "xl"
      }
