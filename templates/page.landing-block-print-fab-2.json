    "16624577622fefe57b": {
      "type": "feature-slides",
      "blocks": {
        "16624577610885c164-0": {
          "type": "text",
          "settings": {
            "heading": "The Jaipur collection",
            "heading_size": "medium",
            "text": "<p>Molly's signature and award-winning collection is based on the vibrant colours seen across Rajasthan's capital city of Jaipur.<\/p>",
            "button_label": "",
            "button_link": "",
