            "image": "shopify:\/\/shop_images\/MOODBOARD_WALLPAPERLANDINGPAGE_FERN_1300X1300_c319c4c6-40b6-4ff0-8de8-d08e0af926a3.jpg",
            "auto_height": "none",
            "cover": false,
            "title": "",
            "subtitle": "",
            "subtitle_size": "small",
            "text": "",
            "button_color": "white",
            "color_scheme": "white_text",
