    "16624601935a72d91c": {
      "type": "2-column-text",
      "blocks": {
        "16624601934254c4eb-0": {
          "type": "text",
          "settings": {
            "heading": "Perfect Imperfections",
            "heading_size": "medium",
            "text": "",
            "button_label": "Learn more about block printing",
            "button_link": "shopify:\/\/pages\/the-art-of-block-printing-with-molly-mahon",
            "inset": true,
