    "166245704164214430": {
      "type": "feature-slides",
      "blocks": {
        "1662457041351d627c-0": {
          "type": "text",
          "settings": {
            "heading": "Blue Hues",
            "heading_size": "medium",
            "text": "<p>Pair text with an image to focus on your chosen product, collection, or blog post. Add details on availability, style, or even provide a review.<\/p>",
            "button_label": "Shop all blue designer wallpapers",
            "button_link": "shopify:\/\/collections\/wallpaper",
            "inset": false,
            "color_scheme": "dark_text",
