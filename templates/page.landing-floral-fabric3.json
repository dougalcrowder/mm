    "16619593053301a27e": {
      "type": "2-column-text",
      "blocks": {
        "1661959305d8047f36-0": {
          "type": "text",
          "settings": {
            "heading": "Bold blooms or dainty ditsy print?",
            "heading_size": "large",
            "text": "",
            "button_label": "",
            "button_link": "",
            "inset": true,
            "color_scheme": "dark_text",
            "desktop_text_box_position": "end",
            "text_align": "left"
          }
        }
      },
