      "blocks": {
        "1642175561bfe5b191-0": {
          "type": "heading",
          "settings": {
            "heading": "Some ways don't change"
          }
        },
        "1642175561bfe5b191-1": {
          "type": "text",
          "settings": {
            "text": "<p>Every stage of our quilt production is done by hand - they are handwoven, hand block printed, hand carded and hand stitched in Jaipur pretty much as they would have been 300 years ago.<\/p>"
          }
        }
      },
      "block_order": [
        "1642175561bfe5b191-0",
        "1642175561bfe5b191-1"
      ],
      "settings": {
